<!doctype html>
<html class="scroll-smooth">
  <head>
    <title>Dreamcode Studios | Demand Gen & Paid Ads for B2B</title>

    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <meta name="description" content="We help B2B companies gain more revenue with optimized paid advertising strategies. No long-term contracts. Minimal oversight from you." />

    <link rel="icon" href="/assets/images/favicons/favicon9.png" type="image/png">

    <link rel="stylesheet" href="{{ '/assets/styles/tailwind.css' | url }}">

    {# This script tag is required to use Tailwind Plus Elements #}
    <script src="https://cdn.jsdelivr.net/npm/@tailwindplus/elements@1" type="module"></script>

    {% if site.env == "production" %}
      {% include "_gtm.njk" %}
    {% endif %}
  </head>
  <body>
    {% if site.env == "production" %}
      {% include "_gtm_noscript.njk" %}
    {% endif %}

    <div class="bg-white dark:bg-gray-900">
      {% include "../home/_header.njk" %}

      <main class="relative isolate">
        {{ content | safe }}
      </main>

      {% include "../home/_footer.njk" %}
    </div>
  </body>
</html>
